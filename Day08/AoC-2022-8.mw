<Group hide-input="false" labelreference="L56" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">calcscore := proc(a,b)global r,c,grid;local ls, rs, us, ds, i;local h := grid[a,b];# sweep downds := 0;for i from a+1 to c do    ds := ds + 1;    if grid[i,b] &gt;= h then         break;    end if;end do;# sweep up
us := 0;
for i from a-1 to 1 by -1 do   
    us := us + 1;
    if grid[i,b] &gt;= h then
         break;
    end if;
end do;# sweep right
rs := 0;
for i from b+1 to r do  
    rs := rs + 1;
    if grid[a,i] &gt;= h then
         break;
    end if;
end do;

# sweep left
ls := 0;
for i from b-1 to 1 by -1 do
    ls := ls + 1;
    if grid[a,i] &gt;= h then
         break;
    end if;
end do;return `*`(us, ls, ds, rs);end proc:</Text-field>
</Input>
</Group>
