</Output>
</Group>
<Group hide-input="false" labelreference="L17" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># recursive BFS - too slow

geodecount := proc(state, bp, timeleft, $)
local nxt;
option cache;
#option trace;

    if timeleft = 0 then
        return state[8];
    end if;

    local nbhd := nbs(state, bp);
    local outs := map(thisproc, nbhd, bp, timeleft-1);
    
    return max(0,outs[]);

end proc:</Text-field>
</Input>
</Group>
