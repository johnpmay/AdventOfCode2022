<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">fastgeocount := proc(bpn)local i, res := gsearch(start, bpn);      if res[2] = [] then return 0;     end if;     local mres := max(map2(op,7,res[2]));     local res2 := select(i-&gt;i[7]=mres, res[2]);     return max( 0, seq( geodecount( res2[i], bpn, 24-res[1]), i=1..nops(res[2])) );end proc;</Text-field>
</Input>
</Group>
