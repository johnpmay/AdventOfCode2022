<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">initgrid():sandstart := [500,0];for c to 1000 doc;ls := Array(sandstart);stopped := false;while ls[2] &lt; yrng[2] do    m := false;    while grid[ls[1], ls[2]+1] = &quot;.&quot; do         ls[2] := ls[2]+1;         m := true;          end do; # down     if grid[ls[1]-1, ls[2]+1] = &quot;.&quot; then
         ls[1] := ls[1]-1;         ls[2] := ls[2]+1;
         m := true;
         next;     end if; # diagL      if grid[ls[1]+1, ls[2]+1] = &quot;.&quot; then
         ls[1] := ls[1]+1;         ls[2] := ls[2]+1;
         m := true;         next;
      end if; # diagR      if m = false then # done moving        grid[ls[1], ls[2]] := &quot;o&quot;;        #printgrid();        stopped := true;        break;      end if;end do;if stopped = false then       break;end if;end do: ans1 := c-1;printgrid();</Text-field>
