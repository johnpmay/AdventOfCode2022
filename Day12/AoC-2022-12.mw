<Group labelreference="L55" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">DijkstraAll := proc(theend, start, grid) # backtrack from theend
local pq, costsofar, curr, nb, newcost, pt, dim; 
uses priqueue;

dim:=rhs~([rtable_dims(grid)]);
costsofar := table(sparse=infinity):

initialize(pq):
insert([0,start], pq):
costsofar[start] := 0:

while pq[0] &lt;&gt; 0 do
   curr := extract(pq)[2];
   if curr = theend then
       return min( seq(seq(ifelse(grid[i,j]=1,costsofar[[i,j]],NULL),i=1..dim[1]),j=1..dim[2]) );
       #return eval(costsofar);   end if;
   # climb at most one
   nb := select(n-&gt;grid[curr[]]&lt;=grid[n[]]+1, nbs(curr[], dim));
   for pt in nb do
       newcost := costsofar[curr] + 1;
       if newcost &lt; costsofar[pt] then
           costsofar[pt] := newcost;
           insert([-newcost,pt], pq);
       end if;
   end do;
end do:

end proc:</Text-field>
</Input>
</Group>
