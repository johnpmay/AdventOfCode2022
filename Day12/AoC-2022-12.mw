<Group labelreference="L53" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nbs := proc(i,j,dim)
local out := NULL;
    if i &lt; dim[1] then
       out := out, [i+1, j];
    end if;
    if j &lt; dim[2] then
       out := out, [i, j+1];
    end if;

    if i &gt; 1 then
      out := out, [i-1,j];
    end if;
    if j &gt; 1 then
       out := out, [i, j-1];
    end if;

    return [out];
end proc:

Dijkstra := proc(start, theend, grid)
local pq, costsofar, curr, nb, newcost, pt, dim; 
uses priqueue;

dim:=rhs~([rtable_dims(grid)]);
costsofar := table(sparse=infinity):

initialize(pq):
insert([0,start], pq):
costsofar[start] := 0:

while pq[0] &lt;&gt; 0 do
   curr := extract(pq)[2];
   if curr = theend then
       return costsofar[theend];
   end if;
   # climb at most one
   nb := select(n-&gt;grid[n[]]&lt;=grid(curr[])+1, nbs(curr[], dim));
   for pt in nb do
       newcost := costsofar[curr] + 1;
       if newcost &lt; costsofar[pt] then
           costsofar[pt] := newcost;
           insert([-newcost,pt], pq);
       end if;
   end do;
end do:

end proc:</Text-field>
</Input>
</Group>
