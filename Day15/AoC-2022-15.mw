<Group hide-input="false" labelreference="L16" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">rowcover := proc(targetrow)global sensors;option remember;local cov, newcov, i;cov := ( map(covered,sensors,targetrow) );cov := sort(cov);
to nops(cov) do
newcov := FAIL:for i from 2 to nops(cov) do
    if cov[i][1] &lt;= cov[i-1][2] or abs(cov[i][1] - cov[i-1][2]) &lt;= 1 then
        newcov := subsop(i=NULL, subsop((i-1)=[min(cov[i-1][1],cov[i][1]),max(cov[i-1][2],cov[i][2])], cov));
        break;
    end if;
end do;
if newcov = FAIL or nops(cov)=1 then
    break;
else
    cov := newcov;
end if;
cov;
end do;return cov;end proc:</Text-field>
</Input>
</Group>
